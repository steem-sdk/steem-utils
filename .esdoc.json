{
  "source": "./src/lib",
  "destination": "./docs",
  "package.json": "./package.json",
  "plugins": [
    {"name": "esdoc-standard-plugin"},
    {"name": "esdoc-unexported-identifier-plugin", "option": {"enable": true}},
    {"name": "esdoc-undocumented-identifier-plugin", "option": {"enable": true}},
    {"name": "esdoc-inject-style-plugin", "option": {"enable": true, "styles": ["./src/styles/esdoc.css"]}},
    {
      "name": "esdoc-brand-plugin",
      "option": {
        "logo": "./src/images/steem-sdk-logo.png",
        "title": "Steem Utils",
        "description": "Swiss-army knife of utilities for Steem data",
        "repository": "https://github.com/steem-sdk/steem-utils",
        "site": "https://steemsdk.com",
        "author": "https://steemsdk.com",
        "image": "https://steemsdk.com/logo.png"
      }
    },
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}}
  ]
}